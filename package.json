{
	"name": "womanup-todo-test",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^13.5.0",
		"dayjs": "^1.11.6",
		"firebase": "^9.14.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-scripts": "5.0.1",
		"web-vitals": "^2.1.4"
	},
	"scripts": {
		"start": "concurrently --kill-others \"less-watch-compiler --config less-watcher.config.json\" \"craco start\"",
		"build": "npm run build:styles && craco build",
		"build:styles": "lessc src/styles/index.less > src/styles/index.css",
		"test": "craco test",
		"eject": "react-scripts eject",
		"deploy": "npm run build && firebase deploy"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@craco/craco": "^7.0.0",
		"concurrently": "^7.6.0",
		"less": "^4.1.3",
		"less-watch-compiler": "^1.16.3"
	}
}
